<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.m_struct">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DiscoverActivity"
            android:label="@string/menu_discover" />
        <activity
            android:name=".PlaylistActivity"
            android:label="@string/menu_playlist" />
        <activity
            android:name=".RadioActivity"
            android:label="@string/menu_radio" />
        <activity
            android:name=".PlayActivity"
            android:label="@string/play_screen"/>

        <activity
            android:name=".MusicListActivity"
            android:label="@string/tracklist_screen"/>
    </application>

</manifest>